; Load a module from a file.
#functor load_module(bound Path FilePath, free Module M) @range(?).

; Access the functions of a module.
#functor module_function(bound Module M, free Function F) @range(*).
#functor module_function(free Module M, bound Function F) @range(.).
#functor module_function(bound Module M, bound Function F) @range(?).

; Access the global variables of a module.
#functor module_variable(bound Module M, free Variable V) @range(*).
#functor module_variable(free Module M, bound Variable V) @range(.).
#functor module_variable(bound Module M, bound Variable V) @range(?).

; Access the blocks of a function.
#functor function_block(bound Function F, free BasicBlock B) @range(*).
#functor function_block(free Function F, bound BasicBlock B) @range(.).
#functor function_block(bound Function F, bound BasicBlock B) @range(?).

; Access the instructions of a function.
#functor function_instruction(bound Function F, free Instruction I) @range(*).
#functor function_instruction(free Function F, bound Instruction I) @range(.).
#functor function_instruction(bound Function F, bound Instruction I) @range(?).

; Access the arguments of a function.
#functor function_argument(bound Function F, free Argument A) @range(*).
#functor function_argument(free Function F, bound Argument A) @range(.).
#functor function_argument(bound Function F, bound Argument A) @range(?).

; Access the instructions of a block.
#functor block_instruction(bound BasicBlock B, free Instruction I) @range(*).
#functor block_instruction(free BasicBlock B, bound Instruction I) @range(.).
#functor block_instruction(bound BasicBlock B, bound Instruction I) @range(?).

; Access the terminator instructions of a block.
#functor block_terminator(bound BasicBlock B, free Instruction I) @range(.).
#functor block_terminator(free BasicBlock B, bound Instruction I) @range(?).
#functor block_terminator(bound BasicBlock B, bound Instruction I) @range(?).

; Access the successors of a block.
#functor block_successor(bound BasicBlock B_from, free BasicBlock B_to) @range(*).
#functor block_successor(free BasicBlock B_from, bound BasicBlock B_to) @range(*).
#functor block_successor(bound BasicBlock B_from, bound BasicBlock B_to) @range(?).

; Access the successors of an instruction.
#functor instruction_successor(bound Instruction I_from, free Instruction I_to) @range(*).
#functor instruction_successor(free Instruction I_from, bound Instruction I_to) @range(*).
#functor instruction_successor(bound Instruction I_from, bound Instruction I_to) @range(?).

